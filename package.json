{
  "name": "@campnetwork/origin",
  "version": "1.1.0",
  "main": "dist/core.cjs",
  "exports": {
    ".": {
      "import": "./dist/core.esm.js",
      "require": "./dist/core.cjs",
      "types": "./dist/core.d.ts"
    },
    "./react": "./dist/react/index.esm.js"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepare": "husky",
    "test": "jest",
    "release": "release-it"
  },
  "module": "dist/core.esm.js",
  "keywords": [
    "camp",
    "sdk",
    "network",
    "social",
    "auth",
    "web3"
  ],
  "author": "Camp Network <campnetwork.xyz>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/campaign-layer/origin-sdk.git"
  },
  "files": [
    "/dist"
  ],
  "type": "module",
  "license": "ISC",
  "description": "Origin SDK",
  "dependencies": {
    "@tanstack/react-query": "^5",
    "@walletconnect/ethereum-provider": "^2.17.2",
    "axios": "^1.7.7",
    "viem": "^2.21.37",
    "wagmi": "^2.12.33"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5",
    "react": ">=18 <20",
    "react-dom": ">=18 <20"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@release-it/conventional-changelog": "^10.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "release-it": "^19.0.5",
    "rollup": "^4.24.3",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "typescript": "^5.6.3",
    "xmlhttprequest": "^1.8.0"
  },
  "release-it": {
    "$schema": "https://unpkg.com/release-it/schema/release-it.json",
    "github": {
      "release": false
    },
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagName": "v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "conventionalcommits",
        "infile": "CHANGELOG.md",
        "header": "# Changelog\n\nAll notable changes to this project will be documented in this file.\n",
        "ignoreRecommendedBump": true
      }
    }
  }
}
